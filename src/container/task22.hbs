<main>
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <br>
    <p class="text-center text-uppercase ">{{description}}</p>
    {{/with}}

    <br>

    {{#with section}}
    
   
    <div class="card card-body">
     {{#with about_us}}   
     {{#with title_list}}
    
        <div class="display-6 text-end">{{main}}</div>
        <h2>{{sub}}</h2>
    {{/with}} 
    
    {{#with button}}
            <div><button type="button" class="btn btn-info">{{text}}</button></div>
    {{/with}}
     {{/with}}       
    </div>

    <br>

    {{#with advantage}}
    <div class="card card-body">
    <div class="display-5 text-muted">{{title}}</div>
    {{#with efficiency}}
    <div class="card card-body">
        <div class="display-4 text-muted">{{title}}</div>
        <h6>{{sub_title}}</h6>
        <p>{{description}}</p>
        <div><button type="button" class="btn btn-outline-warning">{{button}}</button></div>
    </div>
    {{/with}}
    {{#with innovation}}
    <div class="card card-body">
        <div class="display-4 text-muted text-center">{{title}}</div>
        <h6 class="text-center">{{sub_title}}</h6>
        <p class="text-center">{{description}}</p>
        <div><button type="button" class="btn btn-outline-warning">{{button}}</button></div>
    </div>
    {{/with}}

    {{#with collaboration}}
    <div class="card card-body">
        <div class="display-4 text-muted text-end">{{title}}</div>
        <h6 class="text-endr">{{sub_title}}</h6>
        <p class="text-end">{{description}}</p>
        <div><button type="button" class="btn btn-outline-warning">{{button}}</button></div>
    </div>
    {{/with}}
    </div>
    {{/with}}

    <br>
    
    {{#with agreement}}
    <div class="card card-body">
        <div class="display-5">{{title}}</div>
    {{#with info}}
    <div class="card card-body">
        <h5>{{sub_title}}</h5>
    
    {{#with detail}}
    <div class="card card-body">
        <p>{{text}}</p>
        <div><button type="button" class="btn btn-success">{{button}}</button</div>
    </div>
    </div>
    {{/with}}
    {{/with}}  
    </div>
    {{/with}}   
    {{/with}}

</main>